			<element ref="xlf:external-file" />
		</choice>
	</complexType>
	<simpleType name="AttrType_purpose">
		<list>
			<simpleType>
				<union memberTypes="xlf:purposeValueList xlf:XTend" />
			</simpleType>
		</list>
	</simpleType>
	<simpleType name="AttrType_datatype">
		<union memberTypes="xlf:datatypeValueList xlf:XTend" />
	</simpleType>
	<simpleType name="AttrType_restype">
		<union memberTypes="xlf:restypeValueList xlf:XTend" />
	</simpleType>
	<simpleType name="AttrType_alttranstype">
		<union memberTypes="xlf:alttranstypeValueList xlf:XTend" />
	</simpleType>
	<simpleType name="AttrType_context-type">
		<union memberTypes="xlf:context-typeValueList xlf:XTend" />
	</simpleType>
	<simpleType name="AttrType_state">
		<union memberTypes="xlf:stateValueList xlf:XTend" />
	</simpleType>
	<simpleType name="AttrType_state-qualifier">
		<union memberTypes="xlf:state-qualifierValueList xlf:XTend" />
	</simpleType>
	<simpleType name="AttrType_count-type">
		<union memberTypes="xlf:restypeValueList xlf:count-typeValueList xlf:datatypeValueList xlf:stateValueList xlf:state-qualifierValueList xlf:XTend" />
	</simpleType>
	<simpleType name="AttrType_InlineDelimiters">
		<union memberTypes="xlf:InlineDelimitersValueList xlf:XTend" />
	</simpleType>
	<simpleType name="AttrType_InlinePlaceholders">
		<union memberTypes="xlf:InlinePlaceholdersValueList xlf:XTend" />
	</simpleType>
	<simpleType name="AttrType_size-unit">
		<union memberTypes="xlf:size-unitValueList xlf:XTend" />
	</simpleType>
	<simpleType name="AttrType_mtype">
		<union memberTypes="xlf:mtypeValueList xlf:XTend" />
	</simpleType>
	<simpleType name="AttrType_unit">
		<union memberTypes="xlf:unitValueList xlf:XTend" />
	</simpleType>
	<simpleType name="AttrType_priority">
		<union memberTypes="xlf:priorityValueList" />
	</simpleType>
	<simpleType name="AttrType_reformat">
		<union memberTypes="xlf:reformatValueYesNo xlf:reformatValueList" />
