				</annotation>
			</enumeration>
			<enumeration value="javascript">
				<annotation>
					<documentation>
						Indicates JavaScript source file data.
					</documentation>
				</annotation>
			</enumeration>
			<enumeration value="jscript">
				<annotation>
					<documentation>
						Indicates JScript source file data.
					</documentation>
				</annotation>
			</enumeration>
			<enumeration value="layout">
				<annotation>
					<documentation>
						Indicates information relating to formatting.
					</documentation>
				</annotation>
			</enumeration>
			<enumeration value="lisp">
				<annotation>
					<documentation>
						Indicates LISP source file data.
					</documentation>
				</annotation>
