	<simpleType name="AttrType_YesNo">
		<restriction base="xsd:NMTOKEN">
			<enumeration value="yes" />
			<enumeration value="no" />
		</restriction>
	</simpleType>
	<simpleType name="AttrType_Position">
		<restriction base="xsd:NMTOKEN">
			<enumeration value="open" />
			<enumeration value="close" />
		</restriction>
	</simpleType>
	<simpleType name="AttrType_assoc">
		<restriction base="xsd:NMTOKEN">
			<enumeration value="preceding" />
			<enumeration value="following" />
			<enumeration value="both" />
		</restriction>
	</simpleType>
	<simpleType name="AttrType_annotates">
		<restriction base="xsd:NMTOKEN">
			<enumeration value="source" />
			<enumeration value="target" />
			<enumeration value="general" />
		</restriction>
	</simpleType>
	<simpleType name="AttrType_Coordinates">
		<annotation>
			<documentation>
				Values for the attribute 'coord'.
			</documentation>
		</annotation>
		<restriction base="xsd:string">
			<pattern value="(-?\d+|#);(-?\d+|#);(-?\d+|#);(-?\d+|#)" />
		</restriction>
	</simpleType>
	<simpleType name="AttrType_Version">
		<annotation>
			<documentation>
				Version values: 1.0 and 1.1 are allowed for backward compatibility.
			</documentation>
		</annotation>
		<restriction base="xsd:string">
			<enumeration value="1.2" />
			<enumeration value="1.1" />
			<enumeration value="1.0" />
		</restriction>
	</simpleType>
	<!-- Groups -->
	<group name="ElemGroup_TextContent">
		<choice>
			<element ref="xlf:g" />
			<element ref="xlf:bpt" />
			<element ref="xlf:ept" />
			<element ref="xlf:ph" />
			<element ref="xlf:it" />
			<element ref="xlf:mrk" />
			<element ref="xlf:x" />
			<element ref="xlf:bx" />
			<element ref="xlf:ex" />
		</choice>
	</group>
	<attributeGroup name="AttrGroup_TextContent">
		<attribute name="id" type="xsd:string" use="required" />
		<attribute name="xid" type="xsd:string" use="optional" />
		<attribute name="equiv-text" type="xsd:string" use="optional" />
		<anyAttribute namespace="##other" processContents="strict" />
	</attributeGroup>
	<!-- XLIFF Structure -->
	<element name="xliff">
		<complexType>
			<sequence maxOccurs="unbounded">
				<any maxOccurs="unbounded" minOccurs="0" namespace="##other" processContents="strict" />
				<element ref="xlf:file" />
			</sequence>
			<attribute name="version" type="xlf:AttrType_Version" use="required" />
			<attribute ref="xml:lang" use="optional" />
			<anyAttribute namespace="##other" processContents="strict" />
		</complexType>
	</element>
	<element name="file">
		<complexType>
			<sequence>
				<element minOccurs="0" ref="xlf:header" />
				<element ref="xlf:body" />
			</sequence>
			<attribute name="original" type="xsd:string" use="required" />
			<attribute name="source-language" type="xsd:language" use="required" />
			<attribute name="datatype" type="xlf:AttrType_datatype" use="required" />
			<attribute name="tool-id" type="xsd:string" use="optional" />
			<attribute name="date" type="xsd:dateTime" use="optional" />
			<attribute ref="xml:space" use="optional" />
			<attribute name="category" type="xsd:string" use="optional" />
			<attribute name="target-language" type="xsd:language" use="optional" />
			<attribute name="product-name" type="xsd:string" use="optional" />
			<attribute name="product-version" type="xsd:string" use="optional" />
			<attribute name="build-num" type="xsd:string" use="optional" />
			<anyAttribute namespace="##other" processContents="strict" />
		</complexType>
		<unique name="U_group_id">
			<selector xpath=".//xlf:group" />
			<field xpath="@id" />
		</unique>
		<key name="K_unit_id">
			<selector xpath=".//xlf:trans-unit|.//xlf:bin-unit" />
			<field xpath="@id" />
		</key>
		<keyref name="KR_unit_id" refer="xlf:K_unit_id">
			<selector xpath=".//bpt|.//ept|.//it|.//ph|.//g|.//x|.//bx|.//ex|.//sub" />
			<field xpath="@xid" />
		</keyref>
		<key name="K_tool-id">
			<selector xpath="xlf:header/xlf:tool" />
			<field xpath="@tool-id" />
		</key>
		<keyref name="KR_file_tool-id" refer="xlf:K_tool-id">
			<selector xpath="." />
			<field xpath="@tool-id" />
		</keyref>
		<keyref name="KR_phase_tool-id" refer="xlf:K_tool-id">
			<selector xpath="xlf:header/xlf:phase-group/xlf:phase" />
			<field xpath="@tool-id" />
		</keyref>
		<keyref name="KR_alt-trans_tool-id" refer="xlf:K_tool-id">
			<selector xpath=".//xlf:trans-unit/xlf:alt-trans" />
			<field xpath="@tool-id" />
		</keyref>
		<key name="K_count-group_name">
			<selector xpath=".//xlf:count-group" />
			<field xpath="@name" />
		</key>
		<unique name="U_context-group_name">
			<selector xpath=".//xlf:context-group" />
			<field xpath="@name" />
		</unique>
		<key name="K_phase-name">
			<selector xpath="xlf:header/xlf:phase-group/xlf:phase" />
			<field xpath="@phase-name" />
		</key>
		<keyref name="KR_phase-name" refer="xlf:K_phase-name">
			<selector xpath=".//xlf:count|.//xlf:trans-unit|.//xlf:target|.//bin-unit|.//bin-target" />
			<field xpath="@phase-name" />
		</keyref>
		<unique name="U_uid">
			<selector xpath=".//xlf:external-file" />
			<field xpath="@uid" />
		</unique>
	</element>
	<element name="header">
		<complexType>
			<sequence>
				<element minOccurs="0" name="skl" type="xlf:ElemType_ExternalReference" />
				<element minOccurs="0" ref="xlf:phase-group" />
				<choice maxOccurs="unbounded" minOccurs="0">
					<element name="glossary" type="xlf:ElemType_ExternalReference" />
					<element name="reference" type="xlf:ElemType_ExternalReference" />
					<element ref="xlf:count-group" />
					<element ref="xlf:note" />
					<element ref="xlf:tool" />
				</choice>
				<any maxOccurs="unbounded" minOccurs="0" namespace="##other" processContents="strict" />
			</sequence>
		</complexType>
	</element>
	<element name="internal-file">
		<complexType>
			<simpleContent>
				<extension base="xsd:string">
					<attribute name="form" type="xsd:string" />
					<attribute name="crc" type="xsd:NMTOKEN" />
				</extension>
			</simpleContent>
		</complexType>
	</element>
	<element name="external-file">
		<complexType>
			<attribute name="href" type="xsd:string" use="required" />
			<attribute name="crc" type="xsd:NMTOKEN" />
			<attribute name="uid" type="xsd:NMTOKEN" />
		</complexType>
	</element>
	<element name="note">
		<complexType>
			<simpleContent>
				<extension base="xsd:string">
					<attribute ref="xml:lang" use="optional" />
					<attribute default="1" name="priority" type="xlf:AttrType_priority" use="optional" />
					<attribute name="from" type="xsd:string" use="optional" />
					<attribute default="general" name="annotates" type="xlf:AttrType_annotates" use="optional" />
				</extension>
			</simpleContent>
		</complexType>
	</element>
	<element name="phase-group">
		<complexType>
			<sequence maxOccurs="unbounded">
				<element ref="xlf:phase" />
			</sequence>
		</complexType>
	</element>
	<element name="phase">
		<complexType>
			<sequence maxOccurs="unbounded" minOccurs="0">
				<element ref="xlf:note" />
			</sequence>
			<attribute name="phase-name" type="xsd:string" use="required" />
			<attribute name="process-name" type="xsd:string" use="required" />
			<attribute name="company-name" type="xsd:string" use="optional" />
			<attribute name="tool-id" type="xsd:string" use="optional" />
			<attribute name="date" type="xsd:dateTime" use="optional" />
			<attribute name="job-id" type="xsd:string" use="optional" />
			<attribute name="contact-name" type="xsd:string" use="optional" />
			<attribute name="contact-email" type="xsd:string" use="optional" />
			<attribute name="contact-phone" type="xsd:string" use="optional" />
		</complexType>
	</element>
	<element name="count-group">
		<complexType>
			<sequence maxOccurs="unbounded" minOccurs="0">
				<element ref="xlf:count" />
			</sequence>
			<attribute name="name" type="xsd:string" use="required" />
		</complexType>
	</element>
	<element name="count">
		<complexType>
			<simpleContent>
